{
    "manifest_version": 2,
    "name": "SteamScamBans",
    "version": "1.0.0",
    "description": "Provides a list with all banned friends",
    "browser_action": {
      "default_popup": "index.html"
    },
    "permissions": [
        "http://*/"
      ],
    "content_security_policy": "script-src 'self' https://unpkg.com ; object-src 'self'"
}